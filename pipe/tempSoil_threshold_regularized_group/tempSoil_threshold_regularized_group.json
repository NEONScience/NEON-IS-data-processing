{
  "pipeline": {
    "name": "tempSoil_threshold_regularized_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/threshold_regularized_group:21",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m threshold_regularized_group.app" ],
    "env": {
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "REGULARIZED_PATH",
          "repo": "tempSoil_regularized_data",
          "glob": "/prt/*/*/(*)",
          "joinOn": "$1",
          "empty_files": true
        } 
      },
      {
        "pfs": {
          "name": "THRESHOLD_PATH",
          "repo": "tempSoil_threshold_select",
          "glob": "/prt/*/*/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": true,
  "standby": true
}
