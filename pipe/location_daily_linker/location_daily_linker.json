{
    "pipeline": {
        "name": "location_daily_linker"
    },
    "transform": {
        "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
        "image": "quay.io/battelleecology/location_daily_linker:1",
        "cmd": [ "/bin/bash" ],
        "stdin": [ "#!/bin/bash", "python3 -m location_daily_linker.location_daily_linker_main" ],
        "env": {
            "SOURCE_TYPE_INDEX": "3",
            "YEAR_INDEX": "4",
            "MONTH_INDEX": "5",
            "LOCATION_INDEX": "6",
            "OUT_PATH": "/pfs/out",
            "LOG_LEVEL": "DEBUG"
        }
    },
    "input": {
        "pfs": {
            "name": "LOCATION_PATH",
            "repo": "location_active_dates",
            "glob": "/*/*"
        }
    },
    "parallelism_spec": {
        "constant": "8"
    },
    "enable_stats": true,
    "standby": true
}
