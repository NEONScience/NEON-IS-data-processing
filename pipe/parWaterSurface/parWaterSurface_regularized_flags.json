{
  "pipeline": {
    "name": "parWaterSurface_regularized_flags"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.rglr.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirRglr=flags",
      "MethRglr=CybiEc",
      "WndwRglr=Trlg",
      "IdxWndw=IdxWndwMin",
      "RptTimeWndw=FALSE",
      "DropNotNumc=FALSE"
      ],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.27",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "parWaterSurface_calibrated_flags",
      "glob": "/pqs1/*/*/*"
    }
  },
  "enable_stats": false,
    "standby": true,
    "resource_requests": {
      "memory": "20M",
      "cpu": 0.25
    }
}
