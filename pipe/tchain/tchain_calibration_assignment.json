{
  "pipeline": {
    "name": "tchain_calibration_assignment"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.cal.asgn.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirErr=/pfs/out/errored_datums",
      "FileYear=$FILE_YEAR",
      "Arry=TRUE"
    ],
    "image": "quay.io/battelleecology/neon-is-cal-asgn-r:v0.0.7",
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "env": {
      "LOG_LEVEL": "INFO"
    }
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "calibration",
          "glob": "/tchain/*"
        }
      },
      {
        "pfs": {
          "name": "FILE_YEAR",
          "repo": "data_source_tchain_list_years",
          "glob": "/data_years.txt"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "210M",
    "cpu": 0.3
  },
  "parallelism_spec": {
    "constant": "4"
  }
}
