{
  "pipeline": {
    "name": "pressureAir_regularized_data"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.rglr.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirRglr=data",
      "DropNotNumc=FALSE",
      "MethRglr=CybiEc",
      "WndwRglr=Trlg",
      "IdxWndw=IdxWndwMin"
      ],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.27",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    
          "pfs": {
          "name": "DIR_IN",
          "repo": "pressureAir_calibrated_data",
          "glob": "/*/*/*"
        }
    
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "1"
  },
  "resource_requests": {
      "memory": "20M",
      "cpu": 0.25
  }
}
