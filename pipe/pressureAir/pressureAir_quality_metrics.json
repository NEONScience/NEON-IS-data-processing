{
  "pipeline": {
    "name": "pressureAir_quality_metrics"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.qaqc.qm.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "WndwAgr=001|030",
      "WghtAlphBeta=2|1",
      "Thsh=0.2"
      ],
    "image": "quay.io/battelleecology/neon-is-qaqc-qm-r:v0.0.9",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "pressureAir_qaqc_flags_group",
          "glob": "/ptb330a/*/*/*/"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "1"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
