{
  "pipeline": {
    "name": "pressureAir_context_filter"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/context_filter:27",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m context_filter.context_filter_main" ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "CONTEXT": "barometric-pressure",
      "LOG_LEVEL": "INFO",
      "TRIM_INDEX": "3",
      "SOURCE_ID_INDEX": "7",
      "DATA_TYPE_INDEX": "8"
    }
  },
  "input": {
      "pfs": {
        "name": "IN_PATH",
        "repo": "pressureAir_related_location_group",
        "glob": "/*/*/*"
      }
  },
  "enable_stats": false,
  "standby": false,
  "resource_requests": {
    "memory": "20M",
    "cpu": 0.05
  }
}
