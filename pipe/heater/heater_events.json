{
    "pipeline": {
        "name": "heater_events"
    },
    "transform": {
        "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
        "image": "quay.io/battelleecology/event_asset_loader:3",
        "cmd": [ "/bin/bash" ],
        "stdin": [ "#!/bin/bash", "python3 -m event_asset_loader.app" ],
        "env": {
            "OUT_PATH": "/pfs/out",
            "LOG_LEVEL": "DEBUG"
        }
    },
    "input": {
        "pfs": {
            "name": "SOURCE_PATH",
            "repo": "location_asset",
            "glob": "/heater/*/*",
            "empty_files": true
        }
    },
    "enable_stats": false,
    "standby": true
}
