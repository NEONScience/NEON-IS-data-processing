{
    "pipeline": {
        "name": "tempSoil_uncertainty_coefficients"
    },
    "transform": {
        "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
        "image": "quay.io/battelleecology/directory_filter:20",
        "cmd": [ "/bin/bash" ],
        "stdin": [ "#!/bin/bash", "python3 -m directory_filter.app" ],
        "env": {
            "OUT_PATH": "/pfs/out",
            "FILTER_DIR": "uncertainty_coef",
            "LOG_LEVEL": "INFO"
        }
    },
    "input": {
        "pfs": {
                "name": "IN_PATH",
                "repo": "tempSoil_context_filter",
                "glob": "/*/*/*/*",
                "empty_files": true
        }
    },
    "enable_stats": false,
    "standby": true,
    "resource_requests": {
      "memory": "20M",
      "cpu": 0.03
    }
}
