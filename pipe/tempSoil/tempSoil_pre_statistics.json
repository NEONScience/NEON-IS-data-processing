{
  "pipeline": {
    "name": "tempSoil_pre_statistics"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/directory_filter:21",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m directory_filter.app"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "FILTER_DIR": "data,uncertainty_data,uncertainty_coefs",
      "LOG_LEVEL": "INFO",
      "RELATIVE_PATH_INDEX": "3"
    }
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "tempSoil_qaqc_plausibility",
      "glob": "/prt/*/*/*",
      "empty_files": true
    }
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "32M",
    "cpu": 0.01
  }
}