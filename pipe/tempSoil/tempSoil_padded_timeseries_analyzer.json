{
  "pipeline": {
    "name": "tempSoil_padded_timeseries_analyzer"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/padded_timeseries_analyzer:15",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m padded_timeseries_analyzer.padded_timeseries_analyzer.padded_timeseries_analyzer_main" ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "DEBUG",
      "RELATIVE_PATH_INDEX": "3"
    }
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
    "pfs": {
      "name": "DATA_PATH",
      "repo": "tempSoil_timeseries_padder",
      "branch": "master",
      "glob": "/prt/*/*/*"
    }
  },
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
