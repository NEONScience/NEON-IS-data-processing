{
  "pipeline": {
    "name": "tempSoil_regularized"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.rglr.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirErr=/pfs/out/errored_datums",
      "DirRglr=data|uncertainty_data|flags",
      "MethRglr=CybiEc",
      "WndwRglr=Trlg",
      "IdxWndw=IdxWndwMin",
      "RptTimeWndw=FALSE",
      "DropNotNumc=FALSE",
      "DirSubCopy=uncertainty_coef"
    ],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.30",
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "env": {
      "LOG_LEVEL": "INFO",
      "PARALLELIZATION_INTERNAL": "4"
    }
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "tempSoil_context_filter",
      "glob": "/prt/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": false,
  "parallelism_spec": {
    "constant": "1"
  },
  "resource_requests": {
    "memory": "20M",
    "cpu": 0.25
  }
}
