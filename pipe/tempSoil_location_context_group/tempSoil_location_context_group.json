{
  "pipeline": {
    "name": "tempSoil_location_context_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/location_context_group:1",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m location_context_group.app" ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "CONTEXT": "soil",
      "LOG_LEVEL": "DEBUG"
    }
  },
  "input": {
      "pfs": {
        "name": "DATA_PATH",
        "repo": "tempSoil_threshold_regularized_group",
        "glob": "/prt/*/*/*/(*)"
      }
  },
  "enable_stats": true,
  "standby": true
}
