{
    "pipeline": {
        "name": "location_loader"
    },
    "transform": {
        "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
        "image": "quay.io/battelleecology/location_loader:20",
        "cmd": [ "/bin/bash" ],
        "stdin": [ "#!/bin/bash", "python3 -m location_loader.location_loader_main" ],
        "env": {
            "LOCATION_TYPE": "CONFIG",
            "OUT_PATH": "/pfs/out",
            "LOG_LEVEL": "INFO"
        },
        "secrets": [
            {
                "name": "pdr-int-secret",
                "key": "uri",
                "env_var": "DATABASE_URL"
            }
        ]
    },
  "input": {
    "pfs": {
      "repo": "cron_daily_tick",
      "glob": "/",
      "empty_files": true
    }
  },
    "enable_stats": false,
    "standby": true
}
