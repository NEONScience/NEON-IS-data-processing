{
  "pipeline": {
    "name": "relHumidity_quality_metrics"
  },
  "transform": {
    "cmd": ["Rscript","/flow.qaqc.qm.R",
    "DirIn=$DIR_IN",
    "DirOut=/pfs/out",
    "FileSchmQm=$FILE_SCHEMA_QM",
    "WndwAgr=001|030",
    "WghtAlphBeta=2|1",
    "Thsh=0.2",
    "GrpQf1=relativeHumidity:relativeHumidityNullQF|relativeHumidityGapQF|relativeHumidityRangeQF|relativeHumidityStepQF|relativeHumiditySpikeQF|relativeHumidityPersistenceQF|errorStateQF",
    "GrpQf2=temperature:temperatureNullQF|temperatureGapQF|temperatureRangeQF|temperatureStepQF|temperatureSpikeQF|temperaturePersistenceQF|errorStateQF",
    "GrpQf3=dewPoint:dewPointNullQF|dewPointGapQF|dewPointRangeQF|dewPointStepQF|dewPointSpikeQF|dewPointPersistenceQF|errorStateQF"],
    "image": "quay.io/battelleecology/neon-is-qaqc-qm-r:v0.0.11",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "relHumidity_qaqc_flags_group",
          "glob": "/hmp155/*/*/*"
        }
      },
      {
        "pfs": {
          "name": "FILE_SCHEMA_QM",
          "repo": "avro_schemas",
          "glob": "/dp01/relHumidity_dp01_quality_metrics.avsc"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": false,
  "parallelism_spec": {
    "constant": "1"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}