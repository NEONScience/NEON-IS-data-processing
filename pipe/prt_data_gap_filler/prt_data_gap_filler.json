{
  "pipeline": {
    "name": "prt_data_gap_filler"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/data_gap_filler:6",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m data_gap_filler.app"
    ],
    "env": {
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out",
      "OUTPUT_DIRECTORIES": "data,calibration,location,uncertainty_data,uncertainty_coef,flags"
    },
    "secrets": [
      {
        "name": "pdr-int-secret",
        "key": "uri",
        "env_var": "DATABASE_URL"
      }
    ]
  },
  "parallelism_spec": {
    "constant": "8"
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "EMPTY_FILES_PATH",
          "repo": "empty_files",
          "glob": "/prt",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "DATA_PATH",
          "repo": "prt_merge_data_by_location",
          "glob": "/prt/*/*/*",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "LOCATION_PATH",
          "repo": "location_active_dates",
          "glob": "/prt/*/*",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": false
}
