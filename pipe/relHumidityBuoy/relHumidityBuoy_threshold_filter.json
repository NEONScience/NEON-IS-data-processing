{
  "pipeline": {
    "name": "relHumidityBuoy_threshold_filter"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "./flow.thsh.filt.R",
      "DirIn=$threshold",
      "DirOut=/pfs/out",
      "Term=relativeHumidity|temperature|dewPoint",
      "Ctxt=relative-humidity"
    ],
    "image": "quay.io/battelleecology/neon-is-thsh-filt-r:v1.0.0",
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "env": {
      "LOG_LEVEL": "INFO"
    }
  },
  "input": {
    "pfs": {
      "repo": "threshold",
      "glob": "/"
    }
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "13M",
    "cpu": 0.01
  }
}
