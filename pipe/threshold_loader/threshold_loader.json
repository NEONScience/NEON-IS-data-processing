{
    "pipeline": {
        "name": "threshold"
    },
    "transform": {
        "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
        "image": "quay.io/battelleecology/threshold_loader:9",
        "cmd": [ "/bin/bash" ],
        "stdin": [ "#!/bin/bash", "python3 -m threshold_loader.threshold_loader_main" ],
        "env": {
            "OUT_PATH": "/pfs/out",
            "LOG_LEVEL": "INFO"
        },
        "secrets": [
            {
                "name": "pdr-int-secret",
                "key": "uri",
                "env_var": "DATABASE_URL"
            }
        ]
    },
    "input": {
        "cron": {
            "name": "tick",
            "spec": "@daily",
            "overwrite": true
        }
    },
    "enable_stats": false,
    "standby": true 
}
