{
  "pipeline": {
    "name": "tempSoil_context_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/context_filter:22",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m context_filter.app" ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "CONTEXT": "soil",
      "LOG_LEVEL": "DEBUG"
    }
  },
  "input": {
      "pfs": {
        "name": "IN_PATH",
        "repo": "prt_location_filter",
        "glob": "/*/*/*/*"
      }
  },
  "enable_stats": true,
  "standby": true
}
