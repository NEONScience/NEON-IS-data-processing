{
  "pipeline": {
    "name": "windobserverii_data_location_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/data_location_group:1",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m data_location_group.app" ],
    "env": {
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "DATA_PATH",
          "repo": "data_source_windobserverii",
          "glob": "/windobserverii/*/*/*/windobserverii_(*)_*-*-*.avro",
          "joinOn": "$1",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "LOCATION_PATH",
          "repo": "location_asset",
          "glob": "/windobserverii/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": true,
  "standby": true
}
