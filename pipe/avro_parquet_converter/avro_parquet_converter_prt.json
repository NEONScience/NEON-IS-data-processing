{
  "pipeline": {
    "name": "avro_parquet_converter_prt"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/avro_parquet_converter:9",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m avro_parquet_converter.app"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "DEBUG"
    }
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "data_source_prt",
      "glob": "/prt/*/*/*"
    }
  },
  "parallelism_spec": {
    "constant": "8"
  },
  "enable_stats": false,
  "standby": true,
  "pod_patch": "[{\"op\":\"replace\",\"path\":\"/containers/1/resources/requests/memory\",\"value\":\"500M\"},{\"op\":\"replace\",\"path\":\"/containers/1/resources/requests/cpu\",\"value\":\"0.7\"}]"
}
