{
  "pipeline": {
    "name": "avro_parquet_converter_exo2"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/avro_parquet_converter:2",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m avro_parquet_converter.app"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "DEBUG"
    }
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "data_source_exo2",
      "glob": "/exo2/*"
    }
  },
  "enable_stats": false,
  "standby": true 
}
