{
  "pipeline": {
    "name": "waterQuality_padded_timeseries_analyzer_sunav2"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/padded_timeseries_analyzer:11",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m padded_timeseries_analyzer.padded_timeseries_analyzer.app" ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "DEBUG"
    }
  },
  "parallelism_spec": {
    "constant": "2"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
    "pfs": {
      "name": "DATA_PATH",
      "repo": "waterQuality_timeseries_padder_sunav2",
      "branch": "master",
      "glob": "/sunav2/*",
      "lazy": true
    }
  },
  "standby": false,
  "cache_size": "64M",
  "max_queue_size": "1"
}
