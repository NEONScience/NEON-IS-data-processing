{
  "pipeline": {
    "name": "windobserverii_data_location_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/data_location_group:5",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m data_location_group.data_location_group_main" ],
    "env": {
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "SOURCE_ID_INDEX": "7"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "DATA_PATH",
          "repo": "data_source_windobserverii_linkmerge",
          "glob": "/windobserverii/*/*/*/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "LOCATION_PATH",
          "repo": "location_asset",
          "glob": "/windobserverii/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true
}
