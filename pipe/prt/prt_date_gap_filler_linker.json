{
  "pipeline": {
    "name": "prt_date_gap_filler_linker"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/date_gap_filler_linker:1",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m date_gap_filler_linker.app"
    ],
    "env": {
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "LOCATION_INDEX": "7",
      "DATA_TYPE_INDEX": "8",
      "FILENAME_INDEX": "9"
    }
  },
  "parallelism_spec": {
    "constant": "4"
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "prt_date_gap_filler",
      "glob": "/prt/*/*",
      "empty_files": true
    }
  },
  "enable_stats": false,
  "standby": false
}
