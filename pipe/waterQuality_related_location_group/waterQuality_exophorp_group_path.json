{
  "pipeline": {
    "name": "waterQuality_exophorp_group_path"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/location_group_path:3",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m location_group_path.app" ],
    "env": {
      "GROUP": "water-quality-",
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "pfs": {
      "name": "SOURCE_PATH",
      "repo": "exophorp_fill_date_gaps_by_location",
      "glob": "/exophorp/*/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true
}
