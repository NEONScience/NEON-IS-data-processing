{
  "pipeline": {
    "name": "hmp155_data_calibration_linker"
  },
  "transform": {
      "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
      "image": "quay.io/battelleecology/data_calibration_linker:1",
      "cmd": [ "/bin/bash" ],
      "stdin": [ "#!/bin/bash", "python3 -m data_calibration_linker.data_calibration_linker_main" ],
      "env": {
          "OUT_PATH": "/pfs/out",
          "LOG_LEVEL": "DEBUG",
          "DATA_SOURCE_TYPE_INDEX": "3",
          "DATA_SOURCE_ID_INDEX": "4",
          "DATA_YEAR_INDEX": "5",
          "DATA_MONTH_INDEX": "6",
          "DATA_DAY_INDEX": "7",
          "RELATIVE_PATH_INDEX": "3"
      }
  },
  "parallelism_spec": {
    "constant": "4"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
    "union": [
      {
        "pfs": {
          "name": "CALIBRATION_PATH",
          "repo": "hmp155_data_calibration_group",
          "branch": "master",
          "glob": "/hmp155/*/*",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "DATA_PATH",
          "repo": "hmp155_data_calibration_group_path",
          "branch": "master",
          "glob": "/hmp155/*/*",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
