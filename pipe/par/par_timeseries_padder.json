{
  "pipeline": {
    "name": "par_timeseries_padder"
  },
  "transform": {
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-ts-pad:sha-b715f33",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m timeseries_padder.timeseries_padder.variable_pad_main --yearindex 3 --monthindex 4 --dayindex 5 --locindex 8 --subdirindex 9"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "INFO",
      "PAD_DIR": "data",
      "COPY_DIR": "none"
    }
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
    "pfs": {
      "name": "DATA_PATH",
      "repo": "par_threshold_regularized_group",
      "branch": "master",
      "glob": "/*/*/*"
    }
  },
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
