{
  "pipeline": {
    "name": "par_regularized_flags"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "./flow.rglr.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirErr=/pfs/out/errored_datums",
      "DirRglr=flags",
      "MethRglr=CybiEc",
      "WndwRglr=Trlg",
      "IdxWndw=IdxWndwMin",
      "RptTimeWndw=FALSE",
      "DropNotNumc=FALSE"
    ],
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-rglr:v1.2.0",
    "env": {
      "LOG_LEVEL": "INFO"
    }
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "par_calibrated_flags",
      "glob": "/*/*/*/"
    }
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "50M",
    "cpu": 0.1
  }
}
