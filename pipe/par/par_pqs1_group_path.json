{
  "pipeline": {
    "name": "par_pqs1_group_path"
  },
  "transform": {
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-group-path:v1.0.1",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m location_group_path.location_group_path_main"
    ],
    "env": {
      "GROUP": "par-met-",
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "LOCATION_INDEX": "7",
      "DATA_TYPE_INDEX": "8"
    }
  },
  "input": {
    "pfs": {
      "name": "SOURCE_PATH",
      "repo": "pqs1_date_gap_filler",
      "glob": "/pqs1/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true
}
