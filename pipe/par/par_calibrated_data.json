{
  "pipeline": {
    "name": "par_calibrated_data"
  },
  "transform": {
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-dir-filt:v1.0.0",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m directory_filter.app"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "FILTER_DIR": "data,location",
      "LOG_LEVEL": "INFO",
      "RELATIVE_PATH_INDEX": "3"
    }
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "par_related_location_group",
      "glob": "/*/*/*",
      "empty_files": true
    }
  },
  "enable_stats": false,
  "standby": true
}
