{
  "pipeline": {
    "name": "groundwaterPhysical_ucrt_troll_specific"
  },
  "transform": {
    "cmd": ["Rscript","/flow.troll.uncertainty.R","DirIn=$DIR_IN","DirOut=/pfs/out","Context=groundwater","WndwInst=TRUE","WndwAgr=030","DirSubCopy=location|uncertainty_coef","FileSchmData=NA","FileSchmUcrt=NA"],
    "image": "quay.io/battelleecology/neon-is-troll-uncertainty-r:v0.0.7",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "pfs": {
          "name": "DIR_IN",
          "repo": "groundwaterPhysical_qaqc_data_group",
          "glob": "/*/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
      "memory": "20M",
      "cpu": 0.25
  }
}