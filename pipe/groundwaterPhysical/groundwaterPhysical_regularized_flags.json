{
  "pipeline": {
    "name": "groundwaterPhysical_regularized_flags"
  },
  "transform": {
    "cmd": ["Rscript","/flow.rglr.R","DirIn=$DIR_IN","DirOut=/pfs/out","DirRglr=flags","FileSchmRglr=NA","MethRglr=CybiEc","WndwRglr=Trlg","IdxWndw=IdxWndwMin"],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.28",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "groundwaterPhysical_calibrated_flags",
      "glob": "/aquatroll200/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "50M",
    "cpu": 0.1
  }
}
