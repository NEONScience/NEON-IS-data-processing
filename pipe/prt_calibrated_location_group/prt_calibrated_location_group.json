{
  "pipeline": {
    "name": "prt_calibrated_location_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/calibrated_location_group:23",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m calibrated_location_group.app" ],
    "env": {
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "CALIBRATED_PATH",
          "repo": "prt_calibration_conversion",
          "glob": "/prt/*/*/*/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "LOCATION_PATH",
          "repo": "location_assets",
          "glob": "/prt/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": true,
  "standby": true
}
