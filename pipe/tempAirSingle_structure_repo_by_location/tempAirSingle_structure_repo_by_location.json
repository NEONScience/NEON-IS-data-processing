{
  "pipeline": {
    "name": "tempAirSingle_structure_repo_by_location"
  },
  "transform": {
    "cmd": ["Rscript","/flow.loc.repo.strc.R","DirIn=$DIR_IN","DirOut=/pfs/out","Ctxt=aspirated-single","Comb=TRUE"],
    "image": "quay.io/battelleecology/neon-is-loc-repo-strc-r:v0.0.5",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "tempAirSingle_context_group",
      "glob": "/prt/*/*/*/"
      }
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
