{
  "pipeline": {
    "name": "ptb330a_calibrated_location_group"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/calibrated_location_group:26",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m calibrated_location_group.calibrated_location_group_main"
    ],
    "env": {
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "SOURCE_ID_INDEX": "4",
      "YEAR_INDEX": "5",
      "MONTH_INDEX": "6",
      "DAY_INDEX": "7",
      "DATA_TYPE_INDEX": "8"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "CALIBRATED_PATH",
          "repo": "ptb330a_calibrated_location_group_path",
          "glob": "/ptb330a/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "LOCATION_PATH",
          "repo": "location_asset",
          "glob": "/ptb330a/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "50M",
    "cpu": 0.1
  }
}
