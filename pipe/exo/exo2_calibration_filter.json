{
  "pipeline": {
    "name": "exo2_calibration_filter"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.cal.filt.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirSubCopy=data"
    ],
    "image": "quay.io/battelleecology/neon-is-cal-filt-r:v0.0.37",
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "env": {
      "LOG_LEVEL": "INFO"
    }
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "exo2_data_calibration_linker",
      "glob": "/exo2/*/*"
    }
  },
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200M",
    "cpu": 0
  },
  "enable_stats": false,
  "standby": true
}