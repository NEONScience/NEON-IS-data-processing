{
  "pipeline": {
    "name": "exophorp_location_active_dates_assignment"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "./flow.loc.grp.asgn.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirErr=/pfs/out/errored_datums",
      "FileYear=$FILE_YEAR",
      "TypeFile=namedLocation"
    ],
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-loc-grp-asgn:v1.3.1",
    "env": {
      "LOG_LEVEL": "INFO"
    }
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "location_loader",
          "glob": "/exophorp/*"
        }
      },
      {
        "pfs": {
          "name": "FILE_YEAR",
          "repo": "data_source_exophorp_list_years",
          "glob": "/data_years.txt"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "200M",
    "cpu": 1
  },
  "parallelism_spec": {
    "constant": "8"
  }
}
