{
  "pipeline": {
    "name": "data_source_exo2_list_years"
  },
  "transform": {
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "ls $REPO_IN > /pfs/out/data_years.txt"
    ]
  },
  "input": {
    "pfs": {
      "name": "REPO_IN",
      "repo": "data_source_exo2_linkmerge",
      "glob": "/exo2",
      "empty_files": true
    }
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "1K",
    "cpu": 0.01
  }
}
