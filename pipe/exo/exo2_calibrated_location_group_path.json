{
  "pipeline": {
    "name": "exo2_calibrated_location_group_path"
  },
  "transform": {
      "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
      "image": "quay.io/battelleecology/path_order:1",
      "cmd": [ "/bin/bash" ],
      "stdin": [ "#!/bin/bash", "python3 -m path_order.path_order_main" ],
      "env": {
          "OUT_PATH": "/pfs/out",
          "LOG_LEVEL": "DEBUG",
          "PATH_INDICES": "3, 7, 4, 5, 6, 8, 9"
      }
  },
  "parallelism_spec": {
    "constant": "4"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "exo2_calibration_conversion",
      "branch": "master",
      "glob": "/exo2/*/*",
      "empty_files": true
    }
  },
  "enable_stats": false,
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
