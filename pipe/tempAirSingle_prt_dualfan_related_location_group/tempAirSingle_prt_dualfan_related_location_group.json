{
  "pipeline": {
    "name": "tempAirSingle_prt_dualfan_related_location_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/related_location_group:1",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m related_location_group.app" ],
    "env": {
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "SOURCE_PATH",
          "repo": "tempAirSingle_threshold_regularized_group",
          "glob": "/prt/*/*/*/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "GROUP_PATH",
          "repo": "dualfan_structure_repo_by_location",
          "glob": "/dualfan/*/*/*/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true
}
