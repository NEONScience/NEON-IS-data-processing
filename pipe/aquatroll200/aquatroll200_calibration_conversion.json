{
  "pipeline": {
    "name": "aquatroll200_calibration_conversion"
  },
  "transform": {
    "cmd": ["Rscript","/flow.cal.conv.R","DirIn=$DIR_IN","DirOut=/pfs/out","FileSchmData=$FILE_SCHEMA_DATA","FileSchmQf=$FILE_SCHEMA_FLAGS","TermConv=pressure|temperature|conductivity","FuncConv=pressure:def.cal.conv.poly.b|temperature:def.cal.conv.poly|conductivity:def.cal.conv.poly.m","TermQf=pressure|temperature|conductivity","TermUcrt=pressure|temperature|conductivity"],
    "image": "quay.io/battelleecology/neon-is-cal-conv-r:v0.0.32",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "aquatroll200_calibration_filter",
          "glob": "/aquatroll200/*/*/*/"
        }
      },
      {
        "pfs": {
          "name": "FILE_SCHEMA_DATA",
          "repo": "avro_schemas",
          "glob": "/dp0p/aquatroll200_calibrated.avsc"
        }
      },
      {
        "pfs": {
          "name": "FILE_SCHEMA_FLAGS",
          "repo": "avro_schemas",
          "glob": "/dp0p/flags_calibration_aquatroll200.avsc"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "250M",
    "cpu": 0.25
  }
}
