{
  "pipeline": {
    "name": "mti300ahrs_regularized_data"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.rglr.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirErr=/pfs/out/errored_datums",
      "DirRglr=data",
      "MethRglr=CybiEc",
      "WndwRglr=Cntr",
      "IdxWndw=Clst",
      "RptTimeWndw=FALSE",
      "DropNotNumc=FALSE"
      ],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.31",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "mti300ahrs_date_gap_filler_linker",
      "glob": "/mti300ahrs/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": false,
  "resource_requests": {
    "memory": "200M",
    "cpu": 0.3
  }
}
