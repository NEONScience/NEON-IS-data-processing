{
  "pipeline": {
    "name": "tempAirSingle_regularized_flags"
  },
  "transform": {
    "cmd": ["Rscript","/flow.rglr.R","DirIn=$DIR_IN","DirOut=/pfs/out","DirRglr=flags","FileSchmRglr=NA", "FreqRglr=1","MethRglr=CybiEc","WndwRglr=Trlg","IdxWndw=IdxWndwMin"],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.10",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "tempAirSingle_calibrated_flags",
      "glob": "/prt/*/*/*/"
    }
  },
  "enable_stats": true,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
