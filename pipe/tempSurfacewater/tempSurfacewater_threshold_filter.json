{
  "pipeline": {
    "name": "tempSurfacewater_threshold_filter"
  },
  "transform": {
    "cmd": ["Rscript","/flow.thsh.filt.R","DirIn=$threshold","DirOut=/pfs/out","Term=PRTResistance","Ctxt=surfacewater"],
    "image": "quay.io/battelleecology/neon-is-thsh-filt-r:v0.0.7",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "pfs": {
      "repo": "threshold",
      "glob": "/"
    }
  },
  "enable_stats": false,
  "standby": false,
  "resource_requests": {
    "memory": "13M",
    "cpu": 0.01
  }
}