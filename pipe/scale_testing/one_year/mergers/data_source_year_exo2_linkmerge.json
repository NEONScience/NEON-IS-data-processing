{
    "pipeline": {
      "name": "data_source_year_exo2_linkmerge"
    },
    "transform": {
      "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
      "image": "quay.io/battelleecology/parquet_linkmerge:6",
      "cmd": [
        "bash"
      ],
      "stdin": [
        "#!/bin/bash",
        "python3 -m parquet_linkmerge.app"
      ],
      "env": {
        "OUT_PATH": "/pfs/out",
        "LOG_LEVEL": "INFO"
      }
    },
    "parallelism_spec": {
      "constant": "8"
    },
    "output_branch": "master",
    "resource_requests": {
      "memory": "200M",
      "cpu": 0.5
    },
    "input": {
        "pfs": {
          "name": "IN_PATH",
          "repo": "data_source_year_exo2_site",
          "glob": "/*/*/*/*",
          "lazy": true
        }
      },
    "cache_size": "64M",
    "max_queue_size": "1",
    "enable_stats": true,
    "standby": true,
    "pod_patch": "[{\"op\":\"replace\",\"path\":\"/containers/1/resources/requests/memory\",\"value\":\"64M\"},{\"op\":\"replace\",\"path\":\"/containers/1/resources/requests/cpu\",\"value\":\"0.5\"}]"
  }
  
