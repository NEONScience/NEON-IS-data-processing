{
  "pipeline": {
    "name": "data_source_year_exoconductivity_linkmerge"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/parquet_linkmerge:11",
    "cmd": [
      "bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m parquet_linkmerge.parquet_linkmerge_main"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "RELATIVE_PATH_INDEX": "3",
      "LOG_LEVEL": "INFO"
    }
  },
  "parallelism_spec": {
    "constant": "8"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "200M",
    "cpu": 0.5
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "data_source_year_exoconductivity_site",
      "glob": "/*/*/*",
      "lazy": true
    }
  },
  "cache_size": "64M",
  "max_queue_size": "1",
  "enable_stats": true,
  "standby": true,
  "sidecar_resource_limits": {
    "memory": "64M",
    "cpu": "0.5"
  }
}