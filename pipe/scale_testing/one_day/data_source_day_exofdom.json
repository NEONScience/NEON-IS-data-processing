{
  "pipeline": {
    "name": "data_source_day_exofdom"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/grouper:4",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m grouper.app" ],
    "env": {
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "pfs": {
      "name": "DATA_PATH",
      "repo": "data_source_exofdom_linkmerge",
      "glob": "/exofdom/2019/01/05/*",
      "empty_files": true
    }
  },
  "enable_stats": false,
  "standby": true
}
