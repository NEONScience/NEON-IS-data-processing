{
  "pipeline": {
    "name": "data_source_exo2_linkmerge"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/parquet_linkmerge:13",
    "cmd": [
      "bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m parquet_linkmerge.parquet_linkmerge_main"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "INFO",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "SOURCE_ID_INDEX": "7"
    }
  },
  "parallelism_spec": {
    "constant": "4"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "200M",
    "cpu": 0.5
  },
  "input": {
    "pfs": {
      "name": "IN_PATH",
      "repo": "data_source_exo2_site",
      "glob": "/*/*/*",
      "lazy": true
    }
  },
  "cache_size": "64M",
  "max_queue_size": "1",
  "enable_stats": true,
  "standby": true,
  "sidecar_resource_limits": {
    "memory": "64M",
    "cpu": "0.5"
  }
}
