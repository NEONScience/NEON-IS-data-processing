{
  "pipeline": {
    "name": "prt_data_calibration_group"
  },
  "transform": {
      "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
      "image": "quay.io/battelleecology/data_calibration_group:16",
      "cmd": [ "/bin/bash" ],
      "stdin": [ "#!/bin/bash", "python3 -m data_calibration_group.app" ],
      "env": {
          "OUT_PATH": "/pfs/out",
          "LOG_LEVEL": "INFO"
      }
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "CALIBRATION_PATH",
          "repo": "calibration",
          "branch": "master",
          "glob": "/prt/(*)",
          "joinOn": "$1",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "DATA_PATH",
          "repo": "data_source_prt",
          "branch": "master",
          "glob": "/prt/*/*/*/prt_(*)_*-*-*.*",
          "joinOn": "$1",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
