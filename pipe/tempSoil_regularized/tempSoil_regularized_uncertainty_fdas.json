{
  "pipeline": {
    "name": "tempSoil_regularized_uncertainty_fdas"
  },
  "transform": {
    "cmd": ["Rscript","/flow.rglr.R","DirIn=$DIR_IN","DirOut=/pfs/out","DirRglr=uncertainty_fdas","FileSchmRglr=NA", "FreqRglr=0.1","MethRglr=CybiEc","WndwRglr=Trlg","IdxWndw=IdxWndwMin"],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.10",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
      "pfs": {
        "name": "DIR_IN",
        "repo": "tempSoil_uncertainty_fdas",
        "glob": "/prt/*/*/*/"
      }
  },
  "enable_stats": true,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
