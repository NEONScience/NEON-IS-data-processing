{
  "pipeline": {
    "name": "tempAirSingle_related_location_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/related_location_group:6",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m related_location_group.app" ],
    "env": {
      "RELATED_PATHS": "PRT_PATH,DUALFAN_PATH,WIND2D_PATH",
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "PRT_PATH",
          "repo": "tempAirSingle_prt_data_location_group_path",
          "glob": "/prt/(*)/(*)/(*)/(*)",
          "joinOn": "$1/$2/$3/$4",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "DUALFAN_PATH",
          "repo": "tempAirSingle_dualfan_data_location_group_path",
          "glob": "/dualfan/(*)/(*)/(*)/(*)",
          "joinOn": "$1/$2/$3/$4",
          "empty_files": true
        }
      },
      {
        "pfs": {
        "name": "WIND2D_PATH",
        "repo": "tempAirSingle_wind2d_data_location_group_path",
        "glob": "/windobserverii/(*)/(*)/(*)/(*)",
        "joinOn": "$1/$2/$3/$4",
        "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true
}
