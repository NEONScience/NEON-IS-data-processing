{
  "pipeline": {
    "name": "waterQuality_regularized_exo"
  },
  "transform": {
    "cmd": ["Rscript","/flow.rglr.R","DirIn=$DIR_IN","DirOut=/pfs/out","DirRglr=data|flags|uncertainty_data","FileSchmRglr=NA", "FreqRglr=0.0033333333333", "MethRglr=CybiEcTimeMeas","DirSubCopy=uncertainty_coef|location|calibration"],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.16",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
      "pfs": {
        "name": "DIR_IN",
        "repo": "waterQuality_exo",
        "glob": "/*/*/*/"
      }
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
