{
  "pipeline": {
    "name": "surfacewaterPhysical_leveltroll500_elevation"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.troll.elevation.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out"
      ],
    "image": "quay.io/battelleecology/neon-is-troll-elevation-r:v0.0.1",
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "leveltroll500_qaqc_data_group",
          "glob": "/leveltroll500/*/*/*/*"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
