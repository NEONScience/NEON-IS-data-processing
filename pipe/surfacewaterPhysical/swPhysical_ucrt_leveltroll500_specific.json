{
  "pipeline": {
    "name": "swPhysical_ucrt_leveltroll500_specific"
  },
  "transform": {
    "cmd": ["Rscript","/flow.troll.uncertainty.R","DirIn=$DIR_IN","DirOut=/pfs/out","Context=surfacewater","WndwInst=FALSE","WndwAgr=005|030","FileSchmUcrt=$SCHEMA_UCRT", "FileSchmSciStats=$SCHEMA_SCI"],
    "image": "quay.io/battelleecology/neon-is-troll-uncertainty-r:v0.0.13",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "swPhysical_leveltroll500_qaqc_data_group",
          "glob": "/leveltroll500/*/*/*"
        }
      },
      {
        "pfs": {
          "name": "SCHEMA_UCRT",
           "repo": "avro_schemas",
           "glob": "/dp01/surfacewaterPhysical_dp01_leveltroll500_specific_ucrt.avsc"
        }
      },
      {
        "pfs": {
          "name": "SCHEMA_SCI",
           "repo": "avro_schemas",
           "glob": "/dp01/surfacewaterPhysical_dp01_troll_specific_sci_stats.avsc"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": false,
  "parallelism_spec": {
    "constant": "1"
  },
  "resource_requests": {
      "memory": "20M",
      "cpu": 0.25
  }
}