{
  "pipeline": {
    "name": "surfacewaterPhysical_leveltroll500_QM"
  },
  "transform": {
    "cmd": ["Rscript","/flow.qaqc.qm.R","DirIn=$DIR_IN","DirOut=/pfs/out","WndwAgr=005|030","WghtAlphBeta=2|1","Thsh=0.2","GrpQf1=pressure:pressureNullQF|pressureGapQF|pressureRangeQF|pressureStepQF|pressureSpikeQF|pressurePersistenceQF|zeroPressure","GrpQf2=temperature:temperatureNullQF|temperatureGapQF|temperatureRangeQF|temperatureStepQF|temperatureSpikeQF|temperaturePersistenceQF|zeroPressureQF"],
    "image": "quay.io/battelleecology/neon-is-qaqc-qm-r:v0.0.7",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "DEBUG"
    }  
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "leveltroll500_qaqc_flags_group",
          "glob": "/leveltroll500/*/*/*"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
