{
  "pipeline": {
    "name": "sunav2_data_path_order"
  },
  "transform": {
      "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
      "image": "quay.io/battelleecology/data_path_order:2",
      "cmd": [ "/bin/bash" ],
      "stdin": [ "#!/bin/bash", "python3 -m data_path_order.data_path_order_main" ],
      "env": {
          "OUT_PATH": "/pfs/out",
          "LOG_LEVEL": "DEBUG",
          "SOURCE_TYPE_INDEX": "3",
          "YEAR_INDEX": "4",
          "MONTH_INDEX": "5",
          "DAY_INDEX": "6",
          "SOURCE_ID_INDEX": "7"
      }
  },
  "parallelism_spec": {
    "constant": "4"
  },
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
      "pfs": {
        "name": "DATA_PATH",
        "repo": "data_source_sunav2",
        "glob": "/sunav2/*/*",
        "empty_files": true
      }
  },
  "enable_stats": false,
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
