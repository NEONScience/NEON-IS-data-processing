{
  "pipeline": {
    "name": "group_test_list_datum"
  },
  "transform": {
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "echo '---------------------NEW DATUM------------------------'",
      "echo $group_test",
      "ls /pfs/group_test/*/*/*/*"
    ]
  },
  "input": {
    "group":[
      {
        "pfs": {
          "repo": "group_test",
          "glob": "/prt/(*/*/*/)*[[:alnum:][:punct:]]([[:alnum:][:punct:]])",
          "group_by": "$1$2",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true
}
