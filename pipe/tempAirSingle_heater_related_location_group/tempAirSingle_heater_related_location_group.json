{
  "pipeline": {
    "name": "tempAirSingle_heater_related_location_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/event_related_location_group:3",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m event_related_location_group.app" ],
    "env": {
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "DATA_PATH",
          "repo": "tempAirSingle_related_location_group",
          "glob": "/*/*/*/*",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "EVENT_PATH",
          "repo": "tempAirSingle_heater_group_path",
          "glob": "/heater/*",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": false 
}
