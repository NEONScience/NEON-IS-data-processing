{
  "pipeline": {
    "name": "tempAirSingle_regularized_data"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.rglr.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirRglr=data",
      "MethRglr=CybiEc",
      "WndwRglr=Trlg",
      "IdxWndw=IdxWndwMin",
      "RptTimeWndw=FALSE",
      "DropNotNumc=FALSE"
    ],
    "image": "quay.io/battelleecology/neon-is-rglr-r:v0.0.27",
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "env": {
      "LOG_LEVEL": "INFO",
      "PARALLELIZATION_INTERNAL": "4"
    }
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "tempAirSingle_calibrated_data",
      "glob": "/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true,
  "resource_requests": {
    "memory": "200M",
    "cpu": 0.3
  }
}