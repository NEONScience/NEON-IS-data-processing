{
  "pipeline": {
    "name": "tempAirSingle_heater_group_path"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/location_group_path:5",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m location_group_path.app" ],
    "env": {
      "GROUP": "aspirated-single-",
      "LOG_LEVEL": "DEBUG",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "LOCATION_INDEX": "7",
      "DATA_TYPE_INDEX": "8"
    }
  },
  "input": {
    "pfs": {
      "name": "SOURCE_PATH",
      "repo": "heater_data_location_group",
      "glob": "/heater/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true 
}
