{
  "pipeline": {
    "name": "tempAirSingle_merge_qaqc_data"
  },
  "transform": {
    "cmd": ["Rscript","/flow.qaqc.data.comb.R","DirIn=$DIR_IN","DirOut=/pfs/out","DirSubCombData=data"],
    "image": "quay.io/battelleecology/neon-is-qaqc-data-comb-r:v0.0.5",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
      "pfs": {
        "name": "DIR_IN",
        "repo": "tempAirSingle_qaqc_data_group",
        "glob": "/*/*/*/"
      }
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
