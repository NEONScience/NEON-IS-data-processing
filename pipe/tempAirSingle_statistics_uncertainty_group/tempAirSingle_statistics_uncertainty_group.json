{
  "pipeline": {
    "name": "tempAirSingle_statistics_uncertainty_group"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/joiner:1",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m joiner.app" ],
    "env": {
      "RELATED_PATHS": "STATISTICS_PATH,UNCERTAINTY_COEF_PATH,UNCERTAINTY_DATA_PATH",
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out"
    }
  },
  "input": {
    "join": [
      {
        "pfs": {
          "name": "STATISTICS_PATH",
          "repo": "tempAirSingle_merge_qaqc_data",
          "glob": "/*/*/(*)/(*)/prt/*/data",
          "joinOn": "$1/$2",
          "empty_files": true
        } 
      },
      {
        "pfs": {
          "name": "UNCERTAINTY_COEF_PATH",
          "repo": "tempAirSingle_uncertainty_coefficients",
          "glob": "/*/*/(*)/(*)/prt/*/uncertainty_coef",
          "joinOn": "$1/$2",
          "empty_files": true
        }
      },
      {
        "pfs": {
          "name": "UNCERTAINTY_DATA_PATH",
          "repo": "tempAirSingle_regularized_uncertainty_fdas",
          "glob": "/*/*/(*)/(*)/prt/*/uncertainty_data",
          "joinOn": "$1/$2",
          "empty_files": true
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true
}
