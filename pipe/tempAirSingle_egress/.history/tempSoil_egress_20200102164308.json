{
  "pipeline": {
    "name": "tempSoil_egress"
  },
  "transform": {
    "image": "centos",
    "cmd": ["bash"],
    "stdin": ["cp -rs $tempSoil_level1_group /pfs/out"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
    "pfs": {
      "repo": "tempSoil_level1_group",
      "glob": "/*/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true
}
