{
  "pipeline": {
    "name": "tempSoil_egress"
  },
  "transform": {
    "image": "centos",
    "cmd": ["bash"],
    "stdin": ["cp -rs $tempSoil_level1_group /pfs/out"],
    "env": {
      "LOG_LEVEL": "INFO"
    },
    "secrets": [
      {
        "name": "pachyderm-storage-secret",
        "env_var": "AWS_ACCESS_KEY_ID",
        "key": "amazon-id"
      },
      {
        "name": "pachyderm-storage-secret",
        "env_var": "AWS_SECRET_ACCESS_KEY",
        "key": "amazon-secret"
      }
    ]
  },  
  "input": {
    "pfs": {
      "repo": "tempSoil_level1_group",
      "glob": "/*/*/*/*"
    }
  },
  "egress": {
    "URL": "https://int-pub-2.test-s3.data.neonscience.org"
  },
  "enable_stats": false,
  "standby": true
}
