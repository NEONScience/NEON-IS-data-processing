{
  "pipeline": {
    "name": "tempSoil_egress"
  },
  "transform": {
    "image": "centos",
    "cmd": ["bash"],
    "stdin": ["cp -rs $tempSoil_level1_group /pfs/out"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },  
  "input": {
    "pfs": {
      "repo": "tempSoil_level1_group",
      "glob": "/*/*/*/*"
    }
  },
  "secrets": [
    {
      "name": "s3-pub",
      "env_var": "AWS_ACCESS_KEY_ID",
      "key": "access_key_id"
    },
    {
      "name": "s3-pub",
      "env_var": "AWS_SECRET_ACCESS_KEY",
      "key": "secret_access_key"
    },
  ]
},
  "egress": {
    "URL": "s3://test-s3.data.neonscience.org/int-pub-2/tempSoil"
  }
  "enable_stats": false,
  "standby": true
}
