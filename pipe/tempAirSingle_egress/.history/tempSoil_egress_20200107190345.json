{
  "pipeline": {
    "name": "tempSoil_egress"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/timeseries_padder:14",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m egress.egress.app --dateindex 2 --locindex 1 ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "INFO"
    }
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "output_branch": "master",
  "input": {
    "pfs": {
      "name": "DATA_PATH",
      "repo": "tempSoil_level1_group",
      "branch": "master",
      "glob": "/prt/*/*/*"
    }
  },
  "egress": {
    "URL": "s3://int-pub-2/tempSoil"
  },
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
