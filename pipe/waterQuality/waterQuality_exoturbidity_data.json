{
    "pipeline": {
        "name": "waterQuality_exoturbidity_data"
    },
    "transform": {
        "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
        "image": "quay.io/battelleecology/directory_filter:20",
        "cmd": [ "/bin/bash" ],
        "stdin": [ "#!/bin/bash", "python3 -m directory_filter.app" ],
        "env": {
            "OUT_PATH": "/pfs/out",
            "FILTER_DIR": "data",
            "LOG_LEVEL": "INFO"
        }
    },
    "input": {
        "pfs": {
                "name": "IN_PATH",
                "repo": "waterQuality_exoturbidity",
                "glob": "/*/*/*/*",
                "empty_files": true
        }
    },
    "enable_stats": false,
    "standby": true
}
