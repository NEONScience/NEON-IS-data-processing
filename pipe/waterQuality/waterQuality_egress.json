{
  "pipeline": {
    "name": "waterQuality_egress"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/egress:12",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m egress.egress.app --outputname waterQuality --dateindex 2 --locindex 1"
    ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "INFO",
      "EGRESS_URL": "https://storage.googleapis.com/neon-int-publication/egress-test"
    }
  },
  "output_branch": "master",
  "input": {
    "pfs": {
      "name": "DATA_PATH",
      "repo": "waterQuality_level1_group",
      "branch": "master",
      "glob": "/*/*/*"
    }
  },
  "egress": {
    "URL": "gs://neon-int-publication/egress-test"
  },
  "standby": true
}