{
  "pipeline": {
    "name": "waterQuality_exofdom_padded_timeseries_analyzer"
  },
  "transform": {
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-padded-ts-analyzer:v1.0.0",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m padded_timeseries_analyzer.padded_timeseries_analyzer.app" ],
    "env": {
      "OUT_PATH": "/pfs/out",
      "LOG_LEVEL": "INFO",
      "RELATIVE_PATH_INDEX": "3"
    }
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "output_branch": "master",
  "resource_requests": {
    "memory": "64M"
  },
  "input": {
    "pfs": {
      "name": "DATA_PATH",
      "repo": "waterQuality_exofdom_timeseries_padder",
      "branch": "master",
      "glob": "/*/*/*"
    }
  },
  "standby": true,
  "cache_size": "64M",
  "max_queue_size": "1"
}
