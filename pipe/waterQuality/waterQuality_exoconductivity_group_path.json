{
  "pipeline": {
    "name": "waterQuality_exoconductivity_group_path"
  },
  "transform": {
    "image_pull_secrets": [ "battelleecology-quay-read-all-pull-secret" ],
    "image": "quay.io/battelleecology/location_group_path:5",
    "cmd": [ "/bin/bash" ],
    "stdin": [ "#!/bin/bash", "python3 -m location_group_path.app" ],
    "env": {
      "GROUP": "water-quality-",
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "LOCATION_INDEX": "7",
      "DATA_TYPE_INDEX": "8"
    }
  },
  "input": {
    "pfs": {
      "name": "SOURCE_PATH",
      "repo": "exoconductivity_merge_data_by_location",
      "glob": "/exoconductivity/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true
}
