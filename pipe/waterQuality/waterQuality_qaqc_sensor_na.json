{
  "pipeline": {
    "name": "waterQuality_qaqc_sensor_na"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "/flow.qaqc.wq.sens.na.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "FileSchmQf=$SCHEMA_FLAGS",
      "DirTypeSrc=exoconductivity|exodissolvedoxygen|exophorp|exototalalgae|exoturbidity",
      "DirSubAply=uncertainty_data"
      ],
    "image": "quay.io/battelleecology/neon-is-qaqc-wq-sens-na-r:v0.0.4",
    "image_pull_secrets":["battelleecology-quay-read-all-pull-secret"],
    "env": {
      "LOG_LEVEL": "INFO"
    }  
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "waterQuality_exo",
          "glob": "/*/*/*/"
        }
      },
      {
        "pfs": {
            "name": "SCHEMA_FLAGS",
            "repo": "avro_schemas",
            "glob": "/dp0p/flags_wq_sensor_NA.avsc"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
