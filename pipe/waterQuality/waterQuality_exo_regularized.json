{
  "pipeline": {
    "name": "waterQuality_exo_regularized"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "./flow.rglr.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirErr=/pfs/out/errored_datums",
      "DirRglr=data|flags|uncertainty_data",
      "FileSchmRglr=NA",
      "MethRglr=CybiEcTimeMeas",
      "WndwRglr=Trlg",
      "IdxWndw=IdxWndwMin",
      "DirSubCopy=uncertainty_coef|location|calibration",
      "WndwRglr=Trlg",
      "IdxWndw=IdxWndwMin"
    ],
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-rglr:v1.2.1",
    "env": {
      "LOG_LEVEL": "INFO"
    }
  },
  "input": {
    "pfs": {
      "name": "DIR_IN",
      "repo": "waterQuality_qaqc_sensor_na",
      "glob": "/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true
}
