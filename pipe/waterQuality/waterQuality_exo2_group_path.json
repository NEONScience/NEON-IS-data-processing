{
  "pipeline": {
    "name": "waterQuality_exo2_group_path"
  },
  "transform": {
    "image_pull_secrets": [
      "battelleecology-quay-read-all-pull-secret"
    ],
    "image": "quay.io/battelleecology/location_group_path:10",
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "#!/bin/bash",
      "python3 -m location_group_path.location_group_path_main"
    ],
    "env": {
      "GROUP": "water-quality-",
      "LOG_LEVEL": "INFO",
      "OUT_PATH": "/pfs/out",
      "SOURCE_TYPE_INDEX": "3",
      "YEAR_INDEX": "4",
      "MONTH_INDEX": "5",
      "DAY_INDEX": "6",
      "LOCATION_INDEX": "7",
      "DATA_TYPE_INDEX": "8"
    }
  },
  "input": {
    "pfs": {
      "name": "SOURCE_PATH",
      "repo": "exo2_date_gap_filler_linker",
      "glob": "/exo2/*/*/*"
    }
  },
  "enable_stats": false,
  "standby": true
}
