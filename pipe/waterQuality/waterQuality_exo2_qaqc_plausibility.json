{
  "pipeline": {
    "name": "waterQuality_exo2_qaqc_plausibility"
  },
  "transform": {
    "cmd": [
      "Rscript",
      "./flow.qaqc.plau.R",
      "DirIn=$DIR_IN",
      "DirOut=/pfs/out",
      "DirErr=/pfs/out/errored_datums",
      "FileSchmQf=$SCHEMA_FLAGS",
      "TermTest1=sensorDepth:null|gap|range|persistence"
    ],
    "image": "us-central1-docker.pkg.dev/neon-shared-service/neonscience/neon-is-qaqc-plau:v1.1.2",
    "env": {
      "LOG_LEVEL": "INFO"
    }
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "name": "DIR_IN",
          "repo": "waterQuality_exo2_padded_timeseries_analyzer",
          "glob": "/*/*/*"
        }
      },
      {
        "pfs": {
          "name": "SCHEMA_FLAGS",
          "repo": "avro_schemas_waterQuality",
          "glob": "/waterQuality/flags_plausibility_exo2.avsc"
        }
      }
    ]
  },
  "enable_stats": false,
  "standby": true,
  "parallelism_spec": {
    "constant": "2"
  },
  "resource_requests": {
    "memory": "200Mi",
    "cpu": 0
  }
}
